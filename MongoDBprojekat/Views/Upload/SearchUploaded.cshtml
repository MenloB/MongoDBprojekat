@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_LayoutDarkNav.cshtml";
    var url = "/UploadedFiles/" + Request.Cookies[0]["username"] + "/" + Model.Name;
}
<div class="container" style="text-align: center; margin-top: 30px;">
    <h2>@Model.Name</h2>

    <div class="container">
        <div class="flex">
            <a class="btn btn-primary" href="@url">Download</a>
            <a class="btn btn-light" href="#">Share</a>
        </div>
    </div>
</div>